{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "module": "ES2022",
    "moduleResolution": "Node",
    "noEmit": true,
    "paths": {
      "@cy-support/*": ["./test/cypress/support/*"]
    },
    "resolveJsonModule": true,
    "strict": true,
    "target": "ES2022",
    "types": ["node", "vite", "vue"]
  },
  "include": ["./**/*.*"]
}
